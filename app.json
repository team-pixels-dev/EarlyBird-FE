{
  "expo": {
    "name": "얼리버드",
    "slug": "early-bird",
    "version": "1.0.1",
    "runtimeVersion": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "jsEngine" : "hermes",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "hermes_enabled": true,
      "supportsTablet": true,
      "bundleIdentifier": "com.earlybirdteam2024.early-bird",
      "googleServicesFile": "./GoogleService-Info.plist",
      "entitlements": {
        "aps-environment": "production"
      }
    },
    "android": {
      "hermesEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "userInterfaceStyle": "얼리버드"
      },
      "package": "com.earlybirdteam2024.earlybird",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-firebase/messaging",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "eecaa118-753c-406f-ba47-d71ead8eb4d6"
      }
    },
    "owner": "jayou2323",
    "updates": {
      "url": "https://u.expo.dev/eecaa118-753c-406f-ba47-d71ead8eb4d6"
    }
  }
}
